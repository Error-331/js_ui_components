{
  "name": "@webfuturistics/ui_components",
  "version": "0.0.1",
  "description": "Package that holds collection of UI components (styled according to material UI style guides) as well as different themes for them",
  "keywords": [
    "ui",
    "components",
    "material",
    "theme",
    "themes",
    "webfuturistics"
  ],
  "homepage": "https://github.com/owner/project#readme",
  "main": "./lib/index.js",
  "engines": {
    "node": ">=8.11.2"
  },
  "scripts": {
    "install-flow-types": "flow-typed install",
    "prepublish": "rm -rf ./lib/ ; npm run prepublish-babel && npm run prepublish-flow",
    "prepublish-babel": "babel ./src --out-dir ./lib",
    "prepublish-flow": "flow-copy-source ./src ./lib",
    "copy-assets-to-public": "rm -rf ./public/ && mkdir -p ./public && cp -a ./src/assets/. ./public/assets/",
    "start-frontend-style-guide-server": "styleguidist server --config ./src_style_guide/styleguide.config.js"
  },
  "author": {
    "name": "Sergei Selihov",
    "email": "red331@mail.ru",
    "url": "https://github.com/Error-331"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/WebFuturistics/js_design_components.git"
  },
  "license": "MIT",
  "peerDependencies": {
    "@webfuturistics/design_components": "github:WebFuturistics/js_design_components",
    "classnames": ">=2.2.5",
    "react": ">=16.2.0",
    "react-dom": ">=16.2.0",
    "react-jss": ">=8.5.0",
    "redux-form": ">=7.2.0",
    "ramda": ">=0.25.0"
  },
  "devDependencies": {
    "util-fns": "^0.1.3",
    "eslint": ">=4.19.1",
    "flow-bin": "0.73.0",
    "flow-typed": ">=2.4.0",
    "flow-copy-source": ">=1.3.0",
    "webpack": "3.6.0",
    "babel-cli": ">=6.26.0",
    "babel-core": ">=6.26.3",
    "babel-eslint": ">=8.2.3",
    "babel-loader": ">=7.1.4",
    "babel-plugin-add-module-exports": ">=0.2.1",
    "babel-plugin-dev-expression": ">=0.2.1",
    "babel-plugin-transform-object-rest-spread": ">=6.26.0",
    "babel-plugin-transform-runtime": ">=6.23.0",
    "babel-preset-env": ">=1.7.0",
    "babel-preset-stage-2": ">=6.24.1",
    "babel-preset-flow": ">=6.23.0",
    "babel-preset-react": ">=6.24.1",
    "babel-register": ">=6.26.0",
    "babel-runtime": ">=6.26.0",
    "eslint-formatter-pretty": ">=1.3.0",
    "eslint-loader": ">=2.0.0",
    "eslint-plugin-babel": ">=4.1.2",
    "eslint-plugin-flowtype": ">=2.47.1",
    "eslint-plugin-react": ">=7.5.1",
    "react-redux": ">=5.0.6",
    "react-router": ">=4.2.0",
    "react-router-dom": ">=4.2.2",
    "react-styleguidist": ">=7.0.17",
    "redux": ">=3.7.2",
    "redux-immutable": ">=4.0.0",
    "express": ">=4.16.3",
    "immutable": ">=3.8.2"
  }
}
